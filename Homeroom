package mockiagrouper;


public class Homeroom {
  private String name;
  private int id;
  private String[] students;


  public Homeroom(String theName, String[] theStudents, int theId) {
      name = theName;
      id = theId;
      students = theStudents;
  }
  
  public String[] getStudents() {
      return students;
  }
}
